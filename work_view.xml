<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical"
    tools:context=".MainActivity" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/background1"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/listview"
            android:layout_width="match_parent"
            android:layout_height="515dp"
            android:orientation="vertical" >

            <ListView
                android:id="@android:id/list"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:divider="#00000000"
                android:dividerHeight="25px" >
            </ListView>
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/more"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="?attr/history"
            android:clickable="true"
            android:paddingBottom="8.0dip"
            android:paddingTop="8.0dip" >

            <ImageView
                android:id="@+id/more_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="5.0dip"
                android:layout_toLeftOf="@+id/more_text"
                android:clickable="false"
                android:src="@drawable/login_more_up" />

            <Button
                android:id="@+id/dataDelete"
                android:layout_width="38sp"
                android:layout_height="30sp"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="清除"
                android:textSize="8.0sp" />

            <TextView
                android:id="@+id/more_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/dataDelete"
                android:layout_alignBottom="@+id/dataDelete"
                android:layout_centerHorizontal="true"
                android:background="@null"
                android:gravity="center"
                android:maxLines="1"
                android:text="历史记录"
                android:textColor="?attr/listcolor"
                android:textSize="14.0sp" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/MsgView"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:background="?attr/msgText"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/msg"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:gravity="right"
                android:text=""
                android:textColor="#FFA07A"
                android:textSize="30dp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/keybroast"
        android:layout_width="match_parent"
        android:layout_height="320dp"
        android:layout_weight="0.05"
        android:background="?attr/background1"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical" >

            <Button
                android:id="@+id/C"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="C"
                android:textColor="#f58383"
                android:textSize="25sp"
                android:textStyle="bold" >
            </Button>

            
             <Button
                android:id="@+id/sin"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="sin"
                android:textColor="#f58383"
                android:textSize="25dp"
                android:textStyle="bold" >
            </Button>
             <Button
                android:id="@+id/ln"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="ln"
                android:textColor="#f58383"
                android:textSize="25dp"
                android:textStyle="bold" >
            </Button>
           <Button
                android:id="@+id/cheng"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="×"
                android:textColor="#f58383"
                android:textSize="25dp"
                android:textStyle="bold" >
            </Button>
            <Button
                android:id="@+id/seven"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="7"
                android:textSize="25dp" >
            </Button>

            <Button
                android:id="@+id/four"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="4"
                android:textSize="25dp" >
            </Button>

            <Button
                android:id="@+id/one"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="1"
                android:textSize="25dp" >
            </Button>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical" >

            <Button
                android:id="@+id/delete"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="←"
                android:textColor="#f58383"
                android:textSize="25dp"
                android:textStyle="bold" >
            </Button>

            
          <Button
                android:id="@+id/cos"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="cos"
                android:textColor="#f58383"
                android:textSize="25dp"
                android:textStyle="bold" >
            </Button>
           <Button
                android:id="@+id/log"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="log"
                android:textColor="#f58383"
                android:textSize="25dp"
                android:textStyle="bold" >
            </Button>
            <Button
                android:id="@+id/chu"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="÷"
                android:textColor="#f58383"
                android:textSize="25dp"
                android:textStyle="bold" >
            </Button>
            <Button
                android:id="@+id/eight"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="8"
                android:textSize="25dp" >
            </Button>

            <Button
                android:id="@+id/five"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="5"
                android:textSize="25dp" >
            </Button>

            <Button
                android:id="@+id/two"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="2"
                android:textSize="25dp" >
            </Button>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical" >

            <Button
                android:id="@+id/zhengOrfu"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="+/-"
                android:textColor="#f58383"
                android:textSize="25dp" >
            </Button>
        
           
               <Button
                android:id="@+id/tan"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="tan"
                android:textColor="#f58383"
                android:textSize="25dp"
                android:textStyle="bold" >
            </Button>
            <Button
                android:id="@+id/power"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="^"
                android:textColor="#f58383"
                android:textSize="25dp"
                android:textStyle="bold" >
            </Button>
             <Button
                android:id="@+id/jia"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="+"
                android:textColor="#f58383"
                android:textSize="25dp" >
            </Button>
            <Button
                android:id="@+id/nine"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="9"
                android:textSize="25dp" >
            </Button>

            <Button
                android:id="@+id/six"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="6"
                android:textSize="25dp" >
            </Button>

            <Button
                android:id="@+id/three"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="3"
                android:textSize="25dp" >
            </Button>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical" >

            <ToggleButton
                android:id="@+id/changeTheme"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:textSize="20dp" >
            </ToggleButton>

           
           <Button
                android:id="@+id/factorial"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="!"
                android:textColor="#f58383"
                android:textSize="25dp"
                android:textStyle="bold" >
            </Button>
              <Button
                android:id="@+id/sqrt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="√"
                android:textColor="#f58383"
                android:textSize="25dp"
                android:textStyle="bold" >
            </Button>
             <Button
                android:id="@+id/jian"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="-"
                android:textColor="#f58383"
                android:textSize="25sp" >
            </Button>
            <Button
                android:id="@+id/point"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="1"
                android:background="?attr/otherbutton"
                android:gravity="center"
                android:text="."
                android:textColor="#f58383"
                android:textSize="25sp" >
            </Button>

            <Button
                android:id="@+id/zero"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="8dp"
                android:layout_weight="0.84"
                android:background="?attr/zero"
                android:text="0"
                android:textSize="25sp" >
            </Button>
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="?attr/background1"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/equal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
     
            android:layout_margin="5dp"
       android:layout_marginTop="8dp"
          android:layout_alignParentBottom="true"
            android:background="?attr/equals"
            android:text="="
            android:textSize="25dp" />
    </RelativeLayout>

</LinearLayout>